<?xml version="1.0" encoding="UTF-8"?>
<ServerManagerConfiguration>
    <ProxyGroup name="filters">
        <SourceProxy name="PlanarGraphLayout" class="ttkPlanarGraphLayout" label="TTK PlanarGraphLayout">
            <Documentation long_help="TTK planarGraphLayout" short_help="TTK planarGraphLayout">TODO</Documentation>

            <InputProperty name="Input" command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources" />
                    <Group name="filters" />
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkUnstructuredGrid" />
                </DataTypeDomain>
                <InputArrayDomain name="input_array" attribute_type="point" />
                <Documentation>Graph.</Documentation>
            </InputProperty>

            <!--<IntVectorProperty name="UsePointSequence" label="Use Point Sequence" command="SetUsePointSequence" number_of_elements="1" default_values="1">-->
            <!--    <BooleanDomain name="bool" />-->
            <!--    <Documentation>Include point sequence in layout computation.</Documentation>-->
            <!--</IntVectorProperty>-->
            <StringVectorProperty name="SequenceFieldName" command="SetSequenceFieldName" number_of_elements="1" animateable="0" label="Sequence Field" default_values="SequenceIndex">
                <ArrayListDomain name="array_list" input_domain_name="input_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>Sequence scalar field.</Documentation>
                <!--<Hints>-->
                <!--    <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="UsePointSequence" value="1" />-->
                <!--</Hints>-->
            </StringVectorProperty>

            <IntVectorProperty name="UsePointSize" label="Use Point Size" command="SetUsePointSize" number_of_elements="1" default_values="0">
                <BooleanDomain name="bool" />
                <Documentation>Include point size in layout computation.</Documentation>
            </IntVectorProperty>
            <StringVectorProperty name="SizeFieldName" command="SetSizeFieldName" number_of_elements="1" animateable="0" label="Size Field" default_values="Size">
                <ArrayListDomain name="array_list" input_domain_name="input_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>Size scalar field.</Documentation>
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="UsePointSize" value="1" />
                </Hints>
            </StringVectorProperty>

            <IntVectorProperty name="UsePointBranch" label="Use Point Branch" command="SetUsePointBranch" number_of_elements="1" default_values="0">
                <BooleanDomain name="bool" />
                <Documentation>Include point branch in layout computation.</Documentation>
            </IntVectorProperty>
            <StringVectorProperty name="BranchFieldName" command="SetBranchFieldName" number_of_elements="1" animateable="0" label="Branch Field" default_values="BranchId_Points">
                <ArrayListDomain name="array_list" input_domain_name="input_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>Branch scalar field.</Documentation>
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="UsePointBranch" value="1" />
                </Hints>
            </StringVectorProperty>

            <IntVectorProperty name="UsePointLevel" label="Use Point Level" command="SetUsePointLevel" number_of_elements="1" default_values="0">
                <BooleanDomain name="bool" />
                <Documentation>Include point level in layout computation.</Documentation>
            </IntVectorProperty>
            <StringVectorProperty name="LevelFieldName" command="SetLevelFieldName" number_of_elements="1" animateable="0" label="Level Field" default_values="LevelIndex">
                <ArrayListDomain name="array_list" input_domain_name="input_array">
                    <RequiredProperties>
                        <Property name="Input" function="Input" />
                    </RequiredProperties>
                </ArrayListDomain>
                <Documentation>Level scalar field.</Documentation>
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="UsePointLevel" value="1" />
                </Hints>
            </StringVectorProperty>

            <StringVectorProperty name="OutputFieldName" command="SetOutputFieldName" number_of_elements="1" animateable="0" label="Output Field" default_values="Layout">
                <Documentation>Output scalar field.</Documentation>
            </StringVectorProperty>

            <IntVectorProperty name="UseAllCores" label="Use All Cores" command="SetUseAllCores" number_of_elements="1" default_values="1" panel_visibility="advanced">
                <BooleanDomain name="bool" />
                <Documentation>Use all available cores.</Documentation>
            </IntVectorProperty>
            <IntVectorProperty name="ThreadNumber" label="Thread Number" command="SetThreadNumber" number_of_elements="1" default_values="1" panel_visibility="advanced">
                <IntRangeDomain name="range" min="1" max="100" />
                <Hints>
                    <PropertyWidgetDecorator type="GenericDecorator" mode="visibility" property="UseAllCores" value="0" />
                </Hints>
                <Documentation>Thread number.</Documentation>
            </IntVectorProperty>
            <IntVectorProperty name="DebugLevel" label="Debug Level" command="SetdebugLevel_" number_of_elements="1" default_values="3" panel_visibility="advanced">
                <IntRangeDomain name="range" min="0" max="100" />
                <Documentation>Debug level.</Documentation>
            </IntVectorProperty>

            <PropertyGroup panel_widget="Line" label="Input Options">
                <Property name="UsePointSequence" />
                <Property name="SequenceFieldName" />
                <Property name="UsePointSize" />
                <Property name="SizeFieldName" />
                <Property name="UsePointBranch" />
                <Property name="BranchFieldName" />
                <Property name="UsePointLevel" />
                <Property name="LevelFieldName" />
            </PropertyGroup>

            <PropertyGroup panel_widget="Line" label="Output Options">
                <Property name="OutputFieldName" />
            </PropertyGroup>

            <PropertyGroup panel_widget="Line" label="Testing">
                <Property name="UseAllCores" />
                <Property name="ThreadNumber" />
                <Property name="DebugLevel" />
            </PropertyGroup>

            <Hints>
                <ShowInMenu category="TTK - Misc" />
            </Hints>
        </SourceProxy>
    </ProxyGroup>
</ServerManagerConfiguration>
